library(ggplot2)
library(dplyr)
library(Rtsne)
install.packages("Rtsne")
library(ggplot2)
library(dplyr)
library(Rtsne)
data("USArrests")
str(USArrests)
data_scaled <- scale(USArrests)
svd_result <- svd(data_scaled)
# Valores singulares
svd_result$d
var_explained <- svd_result$d^2 / sum(svd_result$d^2)
var_explained
var_acumulada <- cumsum(var_explained)
var_acumulada
reduced_data <- data.frame(svd_result$u[,1:2])
reduced_data$State <- rownames(USArrests)
head(reduced_data)
ggplot(reduced_data, aes(x = X1, y = X2, label = State)) +
geom_point(color = "blue", size = 3) +
geom_text(size = 3, vjust = -0.5) +
theme_minimal() +
labs(title = "SVD - Proyecci贸n a 2 dimensiones",
x = "Componente 1",
y = "Componente 2")
plot(var_explained,
type = "b",
pch = 19,
xlab = "Componente",
ylab = "Proporci贸n de Varianza",
main = "Scree Plot - SVD")
